<!--myown.wxml-->
<view class="container log-list">
    <view class="userinfo  myown-title">
        <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
        <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </view>
    <navigator class="myown-join"  url= '../partake/partake'>
        <text class="iconfont icon-cnlonghubang icon-chang"></text>
        <view class="myown-right">
            <text class="myown-text">我参与的场</text>
            <text class="iconfont icon-youjiantou icon-jiantou"></text>
        </view>
    </navigator>
    <view class="myown-commn">
        <navigator class="myown-join"  url= '../store/store'>
            <text class="iconfont icon-shoucang2 icon-chang icon-shou"></text>
            <view class="myown-right myown-border">
                <text class="myown-text">我的收藏</text>
                <text class="iconfont icon-youjiantou icon-jiantou"></text>
            </view>
        </navigator>
        <navigator class="myown-join"  url= '../linkus/linkus'>
            <text class="iconfont icon-kefu-xuanzhong icon-chang icon-link"></text>
            <view class="myown-right">
                <text class="myown-text">联系我们</text>
                <text class="iconfont icon-youjiantou icon-jiantou"></text>
            </view>
        </navigator>
    </view>
</view>
